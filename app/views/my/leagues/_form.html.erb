<%= turbo_frame_tag "league_form" do %>
  <%= form_with model: @league, url: my_leagues_path do |f| %>
    
    <%= f.label :name, "League Name", class: "form-label" %>
    <%= f.text_field :name, class: "form-control" %>
    

    <%= f.submit "Create League", class: "btn btn-primary" %>
  <% end %>
<% end %>