<div class="match">
  <%= team_logo(match.home) %>
  <h3 class="team"><%= match.home %></h3>
  <p class="date">
    <%= match.home_goals %> - <%= match.away_goals %><br>
    <%= match.scheduled_at.strftime("%b %d, %Y") %>
  </p>
  <h3 class="team"><%= match.away %></h3>
  <%= team_logo(match.away) %>
</div>
